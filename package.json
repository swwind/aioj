{
  "main": "build/main.js",
  "scripts": {
    "start": "node .",
    "serve": "cross-env TEST=true node . & vue-cli-service serve --port 8000",
    "dev": "webpack --config webpack/server.js --progress --mode=development --watch",
    "build": "yarn clean && yarn webpack:4 && yarn build:client && yarn build:ssr && yarn webpack:5 && yarn build:server",
    "webpack:4": "yarn remove webpack; yarn add webpack@webpack-4",
    "webpack:5": "yarn remove webpack; yarn add webpack@latest",
    "build:server": "webpack --config webpack/server.js --progress --mode=production",
    "build:client": "vue-cli-service build",
    "build:ssr": "cross-env SSR=true vue-cli-service build",
    "lint": "eslint --ext .ts,.vue -f ./node_modules/eslint-friendly-formatter src app index.ts",
    "lint:fix": "eslint --ext .ts,.vue -f ./node_modules/eslint-friendly-formatter src app index.ts --fix",
    "test": "./test.sh",
    "clean": "rm -rf dist build",
    "mocha": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha"
  },
  "dependencies": {
    "@koa/cors": "^3.1.0",
