    "cross-env": "^7.0.3",
    "esbuild": "^0.9.2",
    "flv.js": "^1.5.0",
    "katex": "^0.13.0",
    "koa": "^2.13.1",
    "koa-body": "^4.2.0",
    "koa-connect": "^2.1.0",
    "koa-router": "^10.0.0",
    "koa-static": "^5.0.0",
    "koa-websocket": "^6.0.0",
    "less": "^4.1.1",
    "markdown-it": "^12.0.4",
