{
  "name": "aioj-test",
  "version": "0.0.0",
  "scripts": {
    "dev": "cross-env DEV=true node build/main.js",
    "clean": "rm -rf build dist",
    "build": "yarn clean && yarn build:client && yarn build:server && yarn build:backend",
    "build:client": "vite build --ssrManifest --outDir dist/client",
    "build:server": "vite build --ssr src/entry-server.ts --outDir dist/server",
    "build:backend": "webpack --mode=production",
    "serve": "node build/main.js",
    "debug": "cross-env DEV=true node --inspect-brk build/main.js"
  },
